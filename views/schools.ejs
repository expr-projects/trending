                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand page-scroll" href="#page-top">Jhansal India</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="hidden">
                        <a href="#page-top"></a>
                    </li>
                    <li>
                        <a class="page-scroll" href="/">Home</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#academics">Academics</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#gallery">Gallery</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#about">About</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#staff">Staff</a>
                    </li>
                    <li>
                        <a class="page-scroll" href="#contact">Contact</a>
                    </li>
                    <li>
                        <a class="page-scroll" id="myBtn">Login</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>

    <!-- Header -->
    <header style="background-image:url(../img/school-header.jpg)">
        <div class="container" >
            <div class="intro-text" >
                <div class="intro-lead-in">Welcome To Our School Web!</div>
                <div class="intro-heading">It's Nice To Meet You</div>
                <a href="#academics" class="page-scroll btn btn-xl">Tell Me More</a>
            </div>
        </div>
    </header>

    <!-- Home Section -->
    <section id="academics">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Academic</h2>
                    <h3 class="section-subheading text-muted">More About Academics.</h3>
                </div>
            </div>
            <div class="row text-center">
                <div class="col-md-4">
                  <a href="/schools">  <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-graduation-cap fa-stack-1x fa-inverse"></i>
                    </span></a>
                    <h4 class="service-heading">Academic Information</h4>
                    <p class="text-muted">Fee structure , Academic Calender etc.</p>
                </div>
                <div class="col-md-4">
                  <a href="/healthcare">
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-medkit fa-stack-1x fa-inverse"></i>
                    </span>
                  </a>
                    <h4 class="service-heading">Academic Programs</h4>
                    <p class="text-muted">all programes details provided by schools.</p>
                </div>
                <div class="col-md-4">
                  <a href="/grampanchayat">
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-legal fa-stack-1x fa-inverse"></i>
                    </span>
                  </a>
                    <h4 class="service-heading">Examination</h4>
                    <p class="text-muted">Exam and result details.</p>
                </div>
            </div>
        </div>
    </section>

<!-- **************** Start of Gallery Grid Section ************************************-->
    <section id="gallery" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Gallery</h2>
                    <h3 class="section-subheading text-muted">Pics of Heritages we have.</h3>
                </div>
            </div>
            <div class="row">
              <% for(var i=0; i < gallery.length; i++) { %>
                <div class="col-md-4 col-sm-6 portfolio-item">
                    <a  href="#1" onclick="setatt('data:<%= gallery[i].image.contentType %>;base64,<%= gallery[i].image.data %>','<%= gallery[i].headingtext %>','<%= gallery[i].headingdescription %>')" class="portfolio-link" data-toggle="modal">
                        <div class="portfolio-hover">
                            <div class="portfolio-hover-content">
                                <!-- <i class="fa fa-plus fa-3x"></i> -->
                            </div>
                        </div>
                        <img src="data:<%= gallery[i].image.contentType %>;base64,<%= gallery[i].image.data %>" class="img-responsive" alt="">
                    </a>
                    <div class="portfolio-caption">
                        <h4><%= gallery[i].headingtext %></h4>
                        <p class="text-muted"><%= gallery[i].headingdescription %></p>
                    </div>
                </div>
               <% } %>
            </div>
        </div>
    </section>

<!-- *********************** Start of About Section *************************************** -->
    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">About</h2>
                    <h3 class="section-subheading text-muted">History of Jhansal.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <ul class="timeline">
                      <% for(var i=0; i < about.length; i++) { %>
                        <% if (i%2==0) {%>
                         <li>
                      <% } else { %>
                        <li class="timeline-inverted">
                          <% } %>
                            <div class="timeline-image">
                            <a  href="#1" onclick="setatt('data:<%= about[i].image.contentType %>;base64,<%= about[i].image.data %>','<%= about[i].headingtext %>','<%= about[i].headingdescription %>')" class="portfolio-link" data-toggle="modal">
                                 <img class="img-circle img-responsive" src="data:<%= about[i].image.contentType %>;base64,<%= about[i].image.data %>" , width="450", height="450", alt="">
                              </a>
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4 class="subheading"><%= about[i].headingtext %></h4>
                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">
                                      <%= about[i].headingdescription %> </p>
                                </div>
                            </div>
                        </li>
                        <% } %>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <h4>Be Part
                                    <br>Of Our
                                    <br>Story!</h4>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

<!--**************** Start of staff Section ************************************************ -->
    <section id="staff" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Our Amazing Staff</h2>
                    <h3 class="section-subheading text-muted">Dedicated to Staff.</h3>
                </div>
            </div>
            <div class="row">
        <% for(var i=0; i < teamStaff.length; i++) { %>
            <div class="col-sm-4">
                <div class="team-member"  >
                  <a  href="#1" onclick="setatt('data:<%= teamStaff[i].image.contentType %>;base64,<%= teamStaff[i].image.data %>','<%= teamStaff[i].name %>','<%= teamStaff[i].role %>')" class="portfolio-link" data-toggle="modal">
                        <img src="data:<%= teamStaff[i].image.contentType %>;base64,<%= teamStaff[i].image.data %>" class="img-responsive img-circle" alt="">
                        <h4><%= teamStaff[i].name %></h4>
                        <p class="text-muted"><%= teamStaff[i].role %></p>
                    </a>
                        <ul class="list-inline social-buttons">
                          <li><a href=<%= teamStaff[i].t_link %> > <i class="fa fa-twitter"></i></a>
                          </li>
                          <li><a href=<%= teamStaff[i].f_link %> ><i class="fa fa-facebook"></i></a>
                          </li>
                          <li><a href=<%= teamStaff[i].lin_link %> ><i class="fa fa-linkedin"></i></a>
                          </li>
                        </ul>
                 </div>
            </div>
          <% } %>

            </div>

        </div>
    </section>
    <!-- Clients Aside -->
    <aside class="clients">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-6">
                    <a href="#">
                        <img src="img/logos/envato.jpg" class="img-responsive img-centered" alt="">
                    </a>
                </div>
                <div class="col-md-3 col-sm-6">
                    <a href="#">
                        <img src="img/logos/designmodo.jpg" class="img-responsive img-centered" alt="">
                    </a>
                </div>
                <div class="col-md-3 col-sm-6">
                    <a href="#">
                        <img src="img/logos/themeforest.jpg" class="img-responsive img-centered" alt="">
                    </a>
                </div>
                <div class="col-md-3 col-sm-6">
                    <a href="#">
                        <img src="img/logos/creative-market.jpg" class="img-responsive img-centered" alt="">
                    </a>
                </div>
            </div>
        </div>
    </aside>

<!-- ************************************Start of Contact Section *********************************-->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Contact Us</h2>
                    <h3 class="section-subheading text-muted">Feel free to contacts and help us to make it better.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <form name="sentMessage" id="contactForm" novalidate>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Your Name *" id="name" required data-validation-required-message="Please enter your name.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="email" class="form-control" placeholder="Your Email *" id="email" required data-validation-required-message="Please enter your email address.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="tel" class="form-control" placeholder="Your Phone *" id="phone" required data-validation-required-message="Please enter your phone number.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <textarea class="form-control" placeholder="Your Message *" id="message" required data-validation-required-message="Please enter a message."></textarea>
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">Send Message</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
<!-- ************************************start static footer**************************** -->
