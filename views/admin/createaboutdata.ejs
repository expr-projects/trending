    <script>
    $(document).ready(function(){
        $("#myBtn").click(function(){
          var ele =document.getElementById('form1');
          ele.setAttribute("action","/about")
          var e1=document.getElementById('testdemo');
          e1.innerHTML ="Create Entry in About";
          var e2=document.getElementById('headtext');
          e2.innerHTML ="Phase Heading";
          e2.setAttribute("placeholder","Enter Heading");
          var e3=document.getElementById('detai');
          e3.innerHTML ="Description";
          e3.setAttribute("placeholder","Enter Deailed Description")

            $("#myModal").modal();

        });

        $("#myBtn1").click(function(){
          var ele =document.getElementById('form1');
          ele.setAttribute("action","/gallery");
          var e1=document.getElementById('testdemo');
          e1.innerHTML ="Create Entry in Gallery";
          var e2=document.getElementById('headtext');
          e2.innerHTML ="Description";
          e2.setAttribute("placeholder","Enter Heading");
          var e3=document.getElementById('detai');
          e3.innerHTML ="Detailed Description";
          e3.setAttribute("placeholder","Enter Description")
            $("#myModal").modal();
        });

        $("#myBtn2").click(function(){
          var ele =document.getElementById('form1');
          ele.setAttribute("action","/teamStaff");
          var e1=document.getElementById('testdemo');
          e1.innerHTML ="Create Entry For New Member";
          var e2=document.getElementById('headtext');
          e2.innerHTML ="MemberName";
          e2.setAttribute("placeholder","Enter Member Name");
          var e3=document.getElementById('detai');
          e3.innerHTML ="RoleName";
          e3.setAttribute("placeholder","Enter  Description")
            $("#myModal").modal();
        });
    });
